<Window x:Class="Content101.ItemsControls.DataBindLists"
				xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
				xmlns:local="clr-namespace:Content101.ItemsControls"
				xmlns:models='clr-namespace:BigStar.Models'
				mc:Ignorable="d"
				Title="DataBindLists"
				Height="450"
				Width="600">
	<Window.DataContext>
		<models:CitySource  />
	</Window.DataContext>
	<StackPanel>
		<TextBlock Text='ListBox'
							 FontWeight='Bold' />
		<ListBox ItemsSource='{Binding Cities}'>

		</ListBox>
		<ItemsControl ItemsSource='{Binding Cities}' ></ItemsControl>
	</StackPanel>
	<Window.Resources>
		
	</Window.Resources>
</Window>
