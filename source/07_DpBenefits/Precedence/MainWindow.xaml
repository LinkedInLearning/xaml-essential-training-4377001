<Window x:Class="Precedence.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Precedence"
        mc:Ignorable="d"
				Title="MainWindow"
				Height="350"
				Width="525"
				FontSize='22'
				WindowState='Maximized'>
	<StackPanel>
		<Button x:Name='FirstDemoButton'
						Click='FirstDemoButton_Click'
						Content='First Example'
						HorizontalAlignment='Center'
						Margin='10,5'
						MinWidth='240' />

		<Button x:Name='SecondDemoButton'
						Click='SecondDemoButton_Click'
						Content='Second Example'
						HorizontalAlignment='Center'
						Margin='10,5'
						MinWidth='240' />

		<Button x:Name='ThirdDemoButton'
						Click='ThirdDemoButton_Click'
						Content='Third Example'
						HorizontalAlignment='Center'
						Margin='10,5'
						MinWidth='240' />

	</StackPanel>
</Window>