<Window x:Class="UseMergedResources.DoneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UseMergedResources"
        mc:Ignorable="d"
        Title="DoneWindow" Height="800" Width="800">
	
	<UniformGrid Columns='2'>
		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource AccentBrush}' />

		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource BorderBrush}' />
		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource CircularAnimationIconBrush}' />

		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource HorizontalAnimationIconBrush}' />
		<Image Height='240'
					 Source='{StaticResource AlienGroup}' />
		<Image Height='240'
					 Source='{StaticResource MonsterGroup}' />

	</UniformGrid>
</Window>
