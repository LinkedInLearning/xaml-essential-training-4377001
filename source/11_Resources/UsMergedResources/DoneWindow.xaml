<Window x:Class="UseMergedResources.DoneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UseMergedResources"
        mc:Ignorable="d"
        Title="DoneWindow" Height="800" Width="800">
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>

				<ResourceDictionary Source='Merged/Brushes.xaml' />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Window.Resources>
	<UniformGrid Columns='2'>
		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource CircularAnimationIconBrush}' />

		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource HorizontalAnimationIconBrush}' />
		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource AccentBrush}' />

		<Ellipse Width='200'
						 Height='200'
						 Fill='{StaticResource PrimaryBrush}' />
		<Image Height='240' />
		<Image Height='240' />

	</UniformGrid>
</Window>
