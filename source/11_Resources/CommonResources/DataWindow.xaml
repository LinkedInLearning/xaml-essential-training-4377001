<Window x:Class="CommonResources.DataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommonResources"
        mc:Ignorable="d"
        xmlns:Models ='clr-namespace:Models'
        Title="DataWindow" Height="300" Width="300">
  <Window.Resources>
    <Models:Trees x:Key='trees' />
    <DataTemplate x:Key='treeTemplate'>
      <StackPanel>
        <TextBlock Text='{Binding TreeName}' />
      </StackPanel>
    </DataTemplate> 
  </Window.Resources>
    <Grid>
    <ListBox DataContext='{StaticResource trees}' ItemsSource='{Binding}'
             ItemTemplate='{StaticResource treeTemplate}'
             />
  </Grid>
</Window>
