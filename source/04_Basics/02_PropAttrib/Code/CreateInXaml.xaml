<Window x:Class="PropertyAttributes.CreateInXaml"
				xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

				Title="CreateInXaml"
				Height="400"
				Width="400">
	<StackPanel>
		<Rectangle Fill='Orange'
							 Height='90'
							 Width='60'
							 Stroke='Red' StrokeDashArray='6 3' />
		<Ellipse Fill='Purple'
						 Width='200'
						 Height='160'
						 Opacity='.7'
						 Margin='20,-40,20,20' />

	</StackPanel>
</Window>
